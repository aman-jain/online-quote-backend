{
    "globals": {
        "require": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "strict": 0,
        "indent": ["error", 4],
        "prefer-destructuring": 0,
        "no-underscore-dangle": 0,
        "no-restricted-syntax": 0,
        "no-unused-expressions": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "airbnb"
}
